         RowBox[{"res", "=", 
          RowBox[{"LIO", "[", 
           RowBox[{"s", ",", "l", ",", "f"}], "]"}]}], ",", 
         RowBox[{"basecnt", "=", "0"}], ",", "ini", ",", "i"}], "}"}], ",", 
       "\n", "\t", "\n", "\t", 
       RowBox[{
        RowBox[{"While", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"res", "[", 
            RowBox[{"[", "2", "]"}], "]"}], ">", "0"}], ",", 
          RowBox[{
           RowBox[{"res", "=", 
            RowBox[{"Log10", "[", "res", "]"}]}], ";", 
           RowBox[{"basecnt", "++"}]}]}], "]"}], ";", "\n", "\t", 
        RowBox[{"ini", "=", 
         RowBox[{"Power", "[", 
          RowBox[{"\"\<10\>\"", ",", 
           RowBox[{"res", "[", 
            RowBox[{"[", "3", "]"}], "]"}]}], "]"}]}], ";", "\n", "\t", 
        RowBox[{"For", "[", 
         RowBox[{
          RowBox[{"i", "=", 
           RowBox[{"basecnt", "-", "1"}]}], ",", 
          RowBox[{"i", "\[GreaterEqual]", "1"}], ",", 
          RowBox[{"i", "--"}], ",", "\n", "\t\t", 
          RowBox[{
           RowBox[{"ini", "=", 
            RowBox[{"Power", "[", 
             RowBox[{"\"\<10\>\"", ",", "ini"}], "]"}]}], ";"}]}], "\n", "\t",
          "]"}], ";", "\n", "\t", "ini"}]}], "\n", "]"}]}], ";"}], "\n", "\n",
    "\n", 
