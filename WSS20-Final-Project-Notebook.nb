Cell[BoxData[
 RowBox[{"\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{
     RowBox[{"--", 
      RowBox[{"--", 
       RowBox[{"--", 
        RowBox[{"-", 
         RowBox[{"--", 
          RowBox[{"--", "subtraction"}]}]}]}]}]}], " ", "function", " ", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"begins", "--"}], "--"}], "--"}], "--"}], "--"}], "--"}], 
       "--"}], "--"}]}], "-"}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"subtraction", "[", 
     RowBox[{
      RowBox[{"LIO", "[", 
       RowBox[{"1", ",", " ", "0", ",", " ", "f_"}], "]"}], ",", " ", 
      RowBox[{"LIO", "[", 
       RowBox[{"1", ",", " ", "0", ",", " ", "g_"}], "]"}]}], "]"}], " ", ":=",
     " ", 
    RowBox[{"LIO", "[", 
     RowBox[{"1", ",", " ", "0", ",", " ", 
      RowBox[{"f", "-", "g"}]}], "]"}]}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"subtraction", "[", 
      RowBox[{
       RowBox[{"LIO", "[", 
        RowBox[{"1", ",", "l_", ",", "f_"}], "]"}], ",", " ", 
       RowBox[{"LIO", "[", 
        RowBox[{"1", ",", "m_", ",", "g_"}], "]"}]}], "]"}], " ", ":=", " ", 
     RowBox[{"Catch", " ", "@", " ", 
      RowBox[{"Module", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"a", ",", "b", ",", "c"}], "}"}], ",", "\n", "\t", 
        RowBox[{
         RowBox[{
          RowBox[{"a", "[", 
