Cell[TextData[StyleBox["Handling Divisions",
 FontWeight->"Bold"]], "Subsubsection",
 CellChangeTimes->{{3.8037289077888823`*^9, 3.803728927760989*^9}, 
   3.803728994795514*^9, 
   3.803729656788702*^9},ExpressionUUID->"15471f92-aab8-44cc-bc53-\
086422e913d6"],

Cell[TextData[{
 "Since ",
 StyleBox["Division",
  FontSlant->"Italic"],
 " operations of two numbers inside ",
 StyleBox["LevelIndexArithmetic[]",
  FontSlant->"Italic"],
 " function would actually get interpreted as ",
 StyleBox["Multiplication of two LIO[]s with the second one having a negative \
index",
  FontSlant->"Italic"],
 ", as since ",
 StyleBox["negative index of LIO[] is not defined ",
  FontSlant->"Italic"],
 "as per the normal level-index notation system, hence it would throw an ",
 StyleBox["error",
  FontSlant->"Italic"],
 " if Division operation is tried inside one single LevelIndexArithmetic[] \
function."
}], "Text",
 CellChangeTimes->{{3.803729015522792*^9, 3.803729101452334*^9}, {
   3.803729254144351*^9, 3.80372934872663*^9}, {3.803729709967641*^9, 
   3.803729712529757*^9}, 
   3.803751392206554*^9},ExpressionUUID->"bbf5d92a-965e-41ff-85e7-\
e10f406c06a7"],

Cell["The same can be handled in the following manner:", "Text",
 CellChangeTimes->{{3.803729356758733*^9, 
  3.803729371870722*^9}},ExpressionUUID->"68153887-a499-4c51-8d50-\
4b64d7747285"],

Cell[BoxData[
 RowBox[{
  RowBox[{"If", " ", "you", " ", "want", " ", "to", " ", "do", " ", 
   FractionBox[
    SuperscriptBox["10", 
     SuperscriptBox["88", 
      SuperscriptBox["677", "309"]]], 
    SuperscriptBox["10", 
     SuperscriptBox["88", "97"]]]}], ",", " ", 
  RowBox[{
  "simply", " ", "convert", " ", "the", " ", "numerator", " ", "and", " ", 
   "the", " ", "denominator", " ", "each", " ", "to", " ", 
   RowBox[{"LIO", "[", "]"}], "s", " ", "first", " ", "using", " ", 
   RowBox[{"LevelIndexArithmetic", "[", "]"}], " ", "function"}], ",", " ", 
  RowBox[{"and", " ", "then", " ", "do", " ", "the", " ", 
   RowBox[{"Division", "."}]}]}]], "Text",
 CellChangeTimes->{{3.803729384025751*^9, 3.803729474654759*^9}, {
  3.803729604706902*^9, 
  3.803729616648027*^9}},ExpressionUUID->"d4c880f2-ff44-45dc-8317-\
6f99e3e7405a"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"LevelIndexArithmetic", "[", 
    SuperscriptBox["10", 
     SuperscriptBox["88", 
      SuperscriptBox["677", "309"]]], "]"}], "/", 
   RowBox[{"LevelIndexArithmetic", "[", 
    SuperscriptBox["10", 
     SuperscriptBox["88", "97"]], "]"}]}], " ", "//", "FromLIO"}]], "Input",
 CellChangeTimes->{{3.803729477204516*^9, 3.803729517341357*^9}, {
  3.803729548486916*^9, 3.8037295969086933`*^9}},
 CellLabel->"In[85]:=",ExpressionUUID->"0bc02a4a-4218-4b1b-acaa-cd5af1f2fd0f"],

