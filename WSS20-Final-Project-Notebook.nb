   RowBox[{
    RowBox[{
     RowBox[{"Options", "[", "LevelIndexArithmetic", "]"}], "=", 
     RowBox[{"{", 
      RowBox[{"Precision", "\[Rule]", "MachinePrecision"}], "}"}]}], ";"}], 
   "\n", 
